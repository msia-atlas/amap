{% extends 'AmapBundle:Default:layout.html.twig' %}

{% block main -%}
<div class="row">
    <div class ="col-md-12 titre">
        <h1>Responsable</h1>
    </div>
</div>
<div class="row">
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Telephone</th>
                <td>{{ entity.telephone }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>
</div>

        <ul class="record_actions">
    <li>
        <a href="{{ path('responsable') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('responsable_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
