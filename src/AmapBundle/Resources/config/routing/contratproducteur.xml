<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="contratProducteur" path="/">
        <default key="_controller">AmapBundle:ContratProducteur:index</default>
    </route>

    <route id="contratProducteur_show" path="/{id}/show">
        <default key="_controller">AmapBundle:ContratProducteur:show</default>
    </route>

    <route id="contratProducteur_new" path="/new">
        <default key="_controller">AmapBundle:ContratProducteur:new</default>
    </route>

    <route id="contratProducteur_create" path="/create" methods="POST">
        <default key="_controller">AmapBundle:ContratProducteur:create</default>
    </route>

    <route id="contratProducteur_edit" path="/{id}/edit">
        <default key="_controller">AmapBundle:ContratProducteur:edit</default>
    </route>

    <route id="contratProducteur_update" path="/{id}/update" methods="POST|PUT">
        <default key="_controller">AmapBundle:ContratProducteur:update</default>
    </route>

    <route id="contratProducteur_delete" path="/{id}/delete" methods="POST|DELETE">
        <default key="_controller">AmapBundle:ContratProducteur:delete</default>
    </route>

</routes>
