<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="preferenceConsommateur" path="/">
        <default key="_controller">AmapBundle:PreferenceConsommateur:index</default>
    </route>

    <route id="preferenceConsommateur_show" path="/{id}/show">
        <default key="_controller">AmapBundle:PreferenceConsommateur:show</default>
    </route>

    <route id="preferenceConsommateur_new" path="/new">
        <default key="_controller">AmapBundle:PreferenceConsommateur:new</default>
    </route>

    <route id="preferenceConsommateur_create" path="/create" methods="POST">
        <default key="_controller">AmapBundle:PreferenceConsommateur:create</default>
    </route>

    <route id="preferenceConsommateur_edit" path="/{id}/edit">
        <default key="_controller">AmapBundle:PreferenceConsommateur:edit</default>
    </route>

    <route id="preferenceConsommateur_update" path="/{id}/update" methods="POST|PUT">
        <default key="_controller">AmapBundle:PreferenceConsommateur:update</default>
    </route>

    <route id="preferenceConsommateur_delete" path="/{id}/delete" methods="POST|DELETE">
        <default key="_controller">AmapBundle:PreferenceConsommateur:delete</default>
    </route>
     <route id="preferenceConsommateur_save" path="/save" methods="POST">
        <default key="_controller">AmapBundle:PreferenceConsommateur:savePreference</default>
    </route>

</routes>
